<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="dao.Ps_etps_mapper" >
    <select id="select_etpsinfo" parameterType="java.util.HashMap" resultType="pojo.EtpsInfo">
        SELECT * FROM etps_info
        <if test="etps_name !=null">
        WHERE etps_name=#{etps_name}
        </if>
        <if test="user_name !=null">
            WHERE user_name=#{user_name}
        </if>
    </select>
    <update id="update_etpsinfo" parameterType="pojo.User">
        UPDATE etps_info SET
        <if test="contact_name !=null">
            contact_name=#{contact_name},
        </if>
        <if test="contact_mail !=null">
            contact_mail=#{contact_mail},
        </if>
        <if test="contact_address !=null">
            contact_address=#{contact_address},
        </if>
        <if test="contact_phone !=null">
            contact_phone=#{contact_phone},
        </if>
        <if test="etps_name !=null">
            etps_name=#{etps_name},
        </if>
        <if test="etps_login_name !=null">
            etps_login_name=#{etps_login_name},
    </if>
        <if test="etps_login_password !=null">
            etps_login_password=#{etps_login_password},
        </if>
        <if test="user_name !=null">
            user_name=#{user_name},
        </if>
        <if test="status !=null">
            status=#{status},
        </if>
        <if test="alipay_token !=null">
            alipay_token=#{alipay_token},
        </if>
        <if test="sign_channel !=null">
            sign_channel=#{sign_channel},
        </if>
        <if test="modify_time !=null">
            modify_time=#{modify_time},
        </if>
        <if test="sign_channel !=null">
            sign_channel=#{sign_channel},
        </if>
        <if test="alipay_token !=null">
           alipay_token=#{alipay_token}
        </if>
        WHERE 1=1
        <if test="etps_name !=null">
            AND etps_name=#{etps_name}
        </if>
        <if test="user_name !=null">
            AND user_name=#{user_name}
        </if>
    </update>
    <insert id="insert_etpsinfo" parameterType="pojo.EtpsInfo">
        INSERT INTO user (user_name,contact_name,contact_sex,contact_phone,contact_address,etps_name,etps_login_name,etps_login_password,status,create_time,alipay_token,sign_channel,mail)
        VALUES
        (#{user_name},#{contact_name},#{contact_sex},#{contact_phone},#{contact_address},#{etps_name},#{etps_login_name},#{etps_login_password},#{status},#{create_time},#{alipay_token},#{sign_channel},#{mail})
    </insert>
    <delete id="del_etpsinfo" parameterType="java.util.HashMap">
        DELETE FROM etps_info WHERE
        <if test="user_name !=null">
        user_name=#{user_name}
        </if>
        <if test="etps_name !=null">
            etps_name=#{etps_name}
        </if>
    </delete>
</mapper>